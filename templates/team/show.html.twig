        <img src="{{ team.pictureUrl }}" class="img-fluid rounded-circle" alt="rounded circle image"
             style="height: 100%">
    </a>
{% endblock %}

{% block body %}
    <div class="container-sm">
        <h1>Dashboard</h1>
    </div>

{% endblock %}
